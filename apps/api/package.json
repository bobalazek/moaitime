{
  "name": "@myzenbuddy/api",
  "version": "0.0.0",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/main.d.mts",
        "default": "./dist/main.mjs"
      },
      "require": {
        "types": "./dist/main.d.ts",
        "default": "./dist/main.js"
      }
    }
  },
  "main": "./dist/main.js",
  "module": "./dist/main.mjs",
  "types": "./dist/main.d.ts",
  "scripts": {
    "build": "tsup",
    "clean": "pnpm dlx rimraf --glob node_modules --glob .turbo --glob dist",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "serve": "node ./dist/main.js",
    "test": "vitest run",
    "types:generate": "tsc --emitDeclarationOnly --declaration --declarationMap --declarationDir dist"
  },
  "dependencies": {
    "@myzenbuddy/database-core": "workspace:*",
    "@myzenbuddy/database-fixtures": "workspace:*",
    "@myzenbuddy/database-seeds": "workspace:*",
    "@myzenbuddy/database-services": "workspace:*",
    "@myzenbuddy/database-services-testing": "workspace:*",
    "@myzenbuddy/database-testing": "workspace:*",
    "@myzenbuddy/shared-backend": "workspace:*",
    "@myzenbuddy/shared-common": "workspace:*",
    "@myzenbuddy/tsconfig-config": "workspace:*",
    "@nestjs/common": "^10.0.2",
    "@nestjs/core": "^10.0.2",
    "@nestjs/platform-express": "^10.2.10",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.10",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express": "4.18.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "supertest": "^6.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.17.41",
    "@types/supertest": "^2.0.16",
    "typescript": "^5.3.2"
  }
}
