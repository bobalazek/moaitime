{
  "name": "@myzenbuddy/web-e2e",
  "version": "0.0.0",
  "scripts": {
    "clean": "pnpm dlx rimraf --glob node_modules --glob .turbo --glob dist",
    "cypress:open": "pnpm exec cypress open",
    "cypress:run": "pnpm exec cypress run",
    "e2e": "start-server-and-test servers:start \":4200|:3636\" cypress:run",
    "e2e:watch": "start-server-and-test servers:start \":4200|:3636\" cypress:open",
    "lint": "eslint .",
    "servers:start": "cd ../../ && pnpm build && pnpm cli database:reload && concurrently --kill-others \"cd ./apps/web && pnpm serve\" \"cd ./apps/api && pnpm serve\""
  },
  "dependencies": {
    "@myzenbuddy/shared-common": "workspace:*",
    "@myzenbuddy/shared-frontend": "workspace:*",
    "concurrently": "^8.2.2",
    "cypress": "^13.0.0",
    "start-server-and-test": "^2.0.3"
  }
}
