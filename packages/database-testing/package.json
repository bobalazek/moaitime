{
  "name": "@myzenbuddy/database-testing",
  "version": "0.0.0",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "clean": "pnpm dlx rimraf --glob node_modules --glob .turbo --glob dist",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "types:generate": "tsc --emitDeclarationOnly --declaration --declarationMap --declarationDir dist"
  },
  "dependencies": {
    "@myzenbuddy/database-core": "workspace:*",
    "@myzenbuddy/database-fixtures": "workspace:*",
    "@myzenbuddy/database-seeds": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.3.2"
  }
}
