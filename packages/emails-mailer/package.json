{
  "name": "@myzenbuddy/emails-mailer",
  "version": "0.0.0",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./globals.css": "./dist/index.css"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "clean": "pnpm dlx rimraf --glob node_modules --glob .turbo --glob dist",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "types:generate": "tsc --emitDeclarationOnly --declaration --declarationMap --declarationDir dist"
  },
  "dependencies": {
    "@myzenbuddy/emails-core": "workspace:*",
    "@myzenbuddy/shared-backend": "workspace:*",
    "@myzenbuddy/shared-logging": "workspace:*",
    "@react-email/render": "^0.0.10",
    "nodemailer": "^6.9.7",
    "react": "18.2.0"
  },
  "devDependencies": {
    "@myzenbuddy/tailwind-config": "workspace:*",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "18.2.33"
  }
}
