{
  "name": "@myzenbuddy/shared-backend",
  "version": "0.0.0",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "clean": "pnpm dlx rimraf --glob node_modules --glob .turbo --glob dist",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "types:generate": "tsc --emitDeclarationOnly --declaration --declarationMap --declarationDir dist"
  },
  "dependencies": {
    "app-root-path": "^3.1.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "typescript": "^5.3.2"
  }
}
