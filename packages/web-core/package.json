{
  "name": "@moaitime/web-core",
  "version": "0.0.0",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./globals.css": "./dist/index.css"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "clean": "pnpm dlx rimraf --glob node_modules --glob .turbo --glob dist",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "test": "vitest run",
    "types:generate": "tsc --emitDeclarationOnly --declaration --declarationMap --declarationDir dist"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "3.2.2",
    "@moaitime/shared-common": "workspace:*",
    "@moaitime/tsconfig-config": "workspace:*",
    "@moaitime/web-ui": "workspace:*",
    "@moaitime/web-ui-editor": "workspace:*",
    "clsx": "^2.0.0",
    "colord": "^2.9.3",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "framer-motion": "^10.16.4",
    "lodash": "^4.17.21",
    "lucide-react": "^0.307.0",
    "react": "18.2.0",
    "react-confetti-explosion": "^2.1.2",
    "react-dom": "18.2.0",
    "react-icons": "^4.11.0",
    "react-query": "^3.39.3",
    "react-router-dom": "6.20.0",
    "use-debounce": "^10.0.0",
    "usehooks-ts": "^2.9.1",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@moaitime/tailwind-config": "workspace:*",
    "@types/lodash": "^4.14.202",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "autoprefixer": "^10.4.16",
    "postcss": "8.4.21",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.3.2"
  }
}
