{
  "name": "@myzenbuddy/web-core",
  "version": "0.0.0",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./styles.css": "./dist/index.css"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "clean": "pnpm dlx rimraf --glob node_modules --glob .turbo --glob dist",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "types:generate": "tsc --emitDeclarationOnly --declaration --declarationMap --declarationDir dist"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "3.2.2",
    "@myzenbuddy/shared-common": "*",
    "@myzenbuddy/shared-frontend": "*",
    "@myzenbuddy/tsconfig-config": "workspace:*",
    "@myzenbuddy/web-ui": "*",
    "@radix-ui/react-popover": "^1.0.7",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.292.0",
    "react": "18.2.0",
    "react-confetti-explosion": "^2.1.2",
    "react-dom": "18.2.0",
    "react-icons": "^4.11.0",
    "react-router-dom": "6.20.0",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@myzenbuddy/tailwind-config": "workspace:*",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "autoprefixer": "^10.4.16",
    "postcss": "8.4.21",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.3.2"
  }
}
